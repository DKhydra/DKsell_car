  <select id="selectUserByOpenId" resultMap="BaseResultMap" parameterType="java.lang.String" >
    select 
    <include refid="Base_Column_List" />
    from sc_user
    where open_id = #{openId,jdbcType=VARCHAR}
  </select>


  <select id="selectUserAll" resultMap="BaseResultMap" parameterType="com.fnzb.dao.entity.event.UserEvent">
    select
    <include refid="Base_Column_List" />
    from sc_user
    <trim prefix="where" prefixOverrides="and | or">
      <if test="realName != null" >
        real_name like CONCAT('%', #{realName,jdbcType=VARCHAR} , '%')
      </if>
      <if test="isVerification != null" >
        is_verification = #{isVerification,jdbcType=TINYINT}
      </if>
      <if test="openId != null" >
        open_id = #{openId,jdbcType=VARCHAR}
      </if>
      <if test="mobile != null" >
        mobile = #{mobile,jdbcType=BIGINT}
      </if>
      <if test="idNumber != null" >
        id_number =#{idNumber,jdbcType=VARCHAR}
      </if>
      <if test="email != null" >
        email =#{email,jdbcType=VARCHAR}
      </if>
    </trim>
    order by id desc
    limit #{pageIndex},#{pageSize}
  </select>

